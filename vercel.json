{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/signup",
      "dest": "/api/auth/signup.js"
    },
    {
      "src": "/api/auth/login",
      "dest": "/api/auth/login.js"
    },
    {
      "src": "/api/auth/me",
      "dest": "/api/auth/me.js"
    },
    {
      "src": "/api/products",
      "dest": "/api/products.js"
    },
    {
      "src": "/api/contacts",
      "dest": "/api/contacts.js"
    },
    {
      "src": "/api/inquiries",
      "dest": "/api/inquiries.js"
    },
    {
      "src": "/api/seed",
      "dest": "/api/seed.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
